{
  "version": "0.2.0",
  "configurations": [


    {
      "name": "Listen for XDebug",
      "type": "php",
      "request": "launch",
      "hostname": "0.0.0.0",
      "port": 9003,
      "pathMappings": {
        "/var/www/html": "${workspaceRoot}"
      },
      "xdebugSettings": {
        "max_children": 100,
        "max_data": 30000
      }
    }
  ]
}
